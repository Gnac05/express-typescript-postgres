{
  "name": "express-typescript-postgres",
  "version": "0.0.0",
  "description": "Express + TypeScript + TypeORM + PostgreSQL ╰┈➤ Boilerplate",
  "author": "Justin Dah-kenangnon <dah.kenangnon@gmail.com>",
  "license": "GPL",
  "scripts": {
    "start": "npm run build && pm2 start ecosystem.config.js",
    "prod:start": "npm run build && cross-env NODE_ENV=production && pm2 start",
    "prod:restart": "npm run build && cross-env NODE_ENV=production && pm2 restart ecosystem.config.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "code:fix": "npx eslint --fix ./src",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "generate:migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/database/index.ts ./src/features/user/migrations",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run"
  },
  "keywords": [
    "express",
    "typescript",
    "typeorm",
    "postgres",
    "boilerplate",
    "starter-template",
    "starter",
    "starter-kit",
    "postgresql",
    "expressjs"
  ],
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "connect-pg-simple": "^9.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.3",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "twilio": "^4.14.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.17",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/nodemailer": "^6.4.8",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
